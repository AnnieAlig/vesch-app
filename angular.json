{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "vesch-app": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/vesch-app",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss",
              "node_modules/wowjs/css/libs/animate.css",
              "node_modules/@fortawesome/fontawesome-free/css/fontawesome.min.css",
              "node_modules/@fortawesome/fontawesome-free/css/solid.min.css",
              "src/assets/styles/juxtapose.css",
              "node_modules/lightgallery.js/dist/css/lightgallery.min.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "src/assets/scripts/anime.min.js",
              "src/assets/scripts/linemaker.js",
              "node_modules/html2canvas/dist/html2canvas.min.js",
              "node_modules/wowjs/dist/wow.js",
              "src/assets/scripts/juxtapose.js",
              "node_modules/lightgallery.js/dist/js/lightgallery.min.js"
            ]
          },
          "configurations": {
            "ru": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ],
              "outputPath": "dist/ru/",
              "baseHref": "/ru/",
              "i18nFile": "src/locale/messages.ru.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "ru",
              "i18nMissingTranslation": "error",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.ru.ts"
                }
              ]
            },
            "uk": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ],
              "outputPath": "dist/uk/",
              "baseHref": "/uk/",
              "i18nFile": "src/locale/messages.uk.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "uk",
              "i18nMissingTranslation": "error",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.uk.ts"
                }
              ]
            },
            "ru-serve": {
              "optimization": false,
              "sourceMap": true,
              "aot": false,
              "buildOptimizer": false,
              "baseHref": "/ru/",
              "i18nFile": "src/locale/messages.ru.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "ru",
              "i18nMissingTranslation": "error",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.ru.ts"
                }
              ]
            },
            "uk-serve": {
              "optimization": false,
              "sourceMap": true,
              "aot": false,
              "buildOptimizer": false,
              "baseHref": "/uk/",
              "i18nFile": "src/locale/messages.uk.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "uk",
              "i18nMissingTranslation": "error",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.uk.ts"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "vesch-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "vesch-app:build:production"
            },
            "ru": {
              "browserTarget": "vesch-app:build:ru"
            },
            "uk": {
              "browserTarget": "vesch-app:build:uk"
            },
            "ru-serve": {
              "browserTarget": "vesch-app:build:ru"
            },
            "uk-serve": {
              "browserTarget": "vesch-app:build:uk"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "vesch-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "vesch-app-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "vesch-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "vesch-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "vesch-app"
}